{% render "../../templates/license_header_scss.liquid",
		project     : project.attributes.spa_frontend_project_name,
  		description : project.attributes.spa_frontend_project_description,
		authors     : project.attributes.authors,
		copyright   : project.attributes.copyright,
		filename    : 'palette-icon.scss'
		%}
.palette-icon {
	width: 64px;
	height: 64px;
	object-fit: none;
}

.palette-icon-border {
	border: solid black 1px;
	border-radius: 5px;
}

.palette-icon-button {
	border: solid black 1px;
	border-radius: 5px;
	cursor: pointer;
}
.palette-icon-button:hover {
	border: solid black 2px;
}

.palette-icon-unknown {
	object-position: 64px 0px;
}

/* first line */
.palette-icon-user {
	object-position: 0px 0px;
}
.palette-icon-company {
	object-position: -64px 0px;
}
.palette-icon-unit{
	object-position: -128px 0px;
}
.palette-icon-section{
	object-position: -192px 0px;
}
.palette-icon-equipment{
	object-position: -256px 0px;
}
.palette-icon-article{
	object-position: -320px 0px;
}
.palette-icon-intervention{
	object-position: -384px 0px;
}
.palette-icon-workorder{
	object-position: -448px 0px;
}
.palette-icon-supply{
	object-position: -512px 0px;
}
.palette-icon-order{
	object-position: -576px 0px;
}
.palette-icon-supplier{
	object-position: -640px 0px;
}



/* second line */
.palette-icon-category{
	object-position: 0px -64px;
}
.palette-icon-subcategory {
	object-position: -64px -64px;
}
.palette-icon-create{
	object-position: -128px -64px;
}
.palette-icon-delete{
	object-position: -192px -64px;
}
.palette-icon-add{
	object-position: -256px -64px;
}
.palette-icon-remove{
	object-position: -320px -64px;
}
.palette-icon-validate{
	object-position: -384px -64px;
}
.palette-icon-cancel{
	object-position: -448px -64px;
}
.palette-icon-display{
	object-position: -512px -64px;
}
.palette-icon-edit{
	object-position: -576px -64px;
}
.palette-icon-erase{
	object-position: -640px -64px;
}


